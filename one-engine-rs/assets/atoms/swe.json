[
  {
    "id": "swe.echo",
    "kind": "proc",
    "name": "Echo Message",
    "signature": {
      "inputs": ["str"],
      "outputs": ["str"]
    },
    "encodings": {
      "cli": ["echo", "{input}"]
    },
    "observables": ["stdout"],
    "version": "0.1"
  },
  {
    "id": "swe.test",
    "kind": "proc", 
    "name": "Run Tests",
    "signature": {
      "inputs": ["path-dir"],
      "outputs": ["json"]
    },
    "encodings": {
      "cli": ["pytest", "--json-report", "{input}"]
    },
    "observables": ["exit_code", "coverage"],
    "version": "0.1"
  }
]
